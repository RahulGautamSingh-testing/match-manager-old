{
  "packageRules":[
    {
      "matchManagers": ["regex", "node"],
      "groupName": "special-managers"
    }
  ],
  "configMigration": true,
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["index.ts"],
      "matchStrings": [
        "default: '(?<depName>ghcr.io/containerbase/sidecar):(?<currentValue>.*)'"
      ],
      "datasourceTemplate": "docker"
    }
  ],
  "prHourlyLimit": 99
}
